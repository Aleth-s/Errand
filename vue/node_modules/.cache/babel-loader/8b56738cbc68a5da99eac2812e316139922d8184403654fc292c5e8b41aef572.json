{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Manager\",\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      chatUserId: 4\n    };\n  },\n  created() {\n    if (!this.user.id) {\n      this.$router.push('/login');\n    }\n    this.$request.get('/chatInfo/selectUserChat/' + chatUserId).then(res => {\n      this.chatList = res.data || [];\n      document.getElementById('chat-text-end').click();\n    });\n  },\n  methods: {\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}'); // 重新获取下用户的最新信息\n    },\n\n    goToPerson() {\n      if (this.user.role === 'admin') {\n        this.$router.push('/adminPerson');\n      }\n    },\n    logout() {\n      localStorage.removeItem('xm-user');\n      this.$router.push('/login');\n    },\n    chat(userId) {\n      this.$request.post('/chatGroup/add', {\n        chatUserId: userId,\n        userId: this.user.id\n      }).then(res => {\n        this.$router.push('/front/chat');\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","chatUserId","created","id","$router","push","$request","get","then","res","chatList","document","getElementById","click","methods","updateUser","goToPerson","role","logout","removeItem","chat","userId","post"],"sources":["src/views/Manager.vue"],"sourcesContent":["<template>\r\n  <div class=\"manager-container\">\r\n    <!--  头部  -->\r\n    <div class=\"manager-header\">\r\n      <div class=\"manager-header-left\">\r\n        <img src=\"@/assets/imgs/logo.png\" />\r\n        <div class=\"title\">校园跑腿后台管理系统</div>\r\n      </div>\r\n\r\n      <div class=\"manager-header-center\">\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n          <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n          <el-breadcrumb-item :to=\"{ path: $route.path }\">{{ $route.meta.name }}</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n      </div>\r\n\r\n      <div class=\"manager-header-right\">\r\n        <div style=\"margin-right: 17px;\">\r\n          <span @click=\"$router.push('/front/chat')\" style=\"font-size: 16px; color: white; cursor: pointer\"><i class=\"el-icon-chat-dot-round\"></i> 用户消息</span>\r\n          <el-badge :value=\"item.chatNum\" :max=\"99\" v-if=\"item.chatNum > 0 && !activeChatUserId\">\r\n            <span style=\"color: #333\">{{ item.chatUserName }}</span>\r\n          </el-badge>\r\n        </div>\r\n        <el-dropdown placement=\"bottom\">\r\n          <div class=\"avatar\">\r\n            <img :src=\"user.avatar || 'https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png'\" />\r\n            <div>{{ user.name ||  '管理员' }}</div>\r\n          </div>\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item @click.native=\"goToPerson\">个人信息</el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"$router.push('/password')\">修改密码</el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"logout\">退出登录</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n      </div>\r\n    </div>\r\n\r\n    <!--  主体  -->\r\n    <div class=\"manager-main\">\r\n      <!--  侧边栏  -->\r\n      <div class=\"manager-main-left\">\r\n        <el-menu :default-openeds=\"['info', 'user']\" router style=\"border: none\" :default-active=\"$route.path\">\r\n          <el-menu-item index=\"/home\">\r\n            <i class=\"el-icon-s-home\"></i>\r\n            <span slot=\"title\">系统首页</span>\r\n          </el-menu-item>\r\n          <el-submenu index=\"info\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>信息管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/notice\">公告信息</el-menu-item>\r\n            <el-menu-item index=\"/orders\">订单信息</el-menu-item>\r\n            <el-menu-item index=\"/address\">地址信息</el-menu-item>\r\n            <el-menu-item index=\"/certification\">骑手认证</el-menu-item>\r\n            <el-menu-item index=\"/records\">收支信息</el-menu-item>\r\n            <el-menu-item index=\"/comment\">用户评价</el-menu-item>\r\n            <el-menu-item index=\"/forum\">校园墙信息</el-menu-item>\r\n            <el-menu-item index=\"/transaction\">二手交易信息</el-menu-item>\r\n            <el-menu-item index=\"/lost\">失物招领信息</el-menu-item>\r\n            <el-menu-item index=\"/chatinfo\">客服聊天信息</el-menu-item>\r\n            <el-menu-item index=\"/comment\">用户评价</el-menu-item>\r\n            <el-menu-item index=\"/report\">举报信息</el-menu-item>\r\n\r\n          </el-submenu>\r\n\r\n          <el-submenu index=\"user\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>用户管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/admin\">管理员信息</el-menu-item>\r\n            <el-menu-item index=\"/user\">用户信息</el-menu-item>\r\n          </el-submenu>\r\n        </el-menu>\r\n      </div>\r\n\r\n      <!--  数据表格  -->\r\n      <div class=\"manager-main-right\">\r\n        <router-view @update:user=\"updateUser\" />\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Manager\",\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      chatUserId:4\r\n    }\r\n  },\r\n  created() {\r\n    if (!this.user.id) {\r\n      this.$router.push('/login')\r\n    }\r\n    this.$request.get('/chatInfo/selectUserChat/' + chatUserId).then(res => {\r\n      this.chatList = res.data || []\r\n      document.getElementById('chat-text-end').click()\r\n    })\r\n  },\r\n  methods: {\r\n    updateUser() {\r\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}')   // 重新获取下用户的最新信息\r\n    },\r\n    goToPerson() {\r\n      if (this.user.role === 'admin') {\r\n        this.$router.push('/adminPerson')\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem('xm-user')\r\n      this.$router.push('/login')\r\n    },\r\n    chat(userId) {\r\n      this.$request.post('/chatGroup/add', { chatUserId: userId, userId: this.user.id }).then(res => {\r\n        this.$router.push('/front/chat')\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import \"@/assets/css/manager.css\";\r\n</style>"],"mappings":";AAqFA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACA,UAAAN,IAAA,CAAAO,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACA,KAAAC,QAAA,CAAAC,GAAA,+BAAAN,UAAA,EAAAO,IAAA,CAAAC,GAAA;MACA,KAAAC,QAAA,GAAAD,GAAA,CAAAd,IAAA;MACAgB,QAAA,CAAAC,cAAA,kBAAAC,KAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAnB,IAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;;IACAgB,WAAA;MACA,SAAApB,IAAA,CAAAqB,IAAA;QACA,KAAAb,OAAA,CAAAC,IAAA;MACA;IACA;IACAa,OAAA;MACAnB,YAAA,CAAAoB,UAAA;MACA,KAAAf,OAAA,CAAAC,IAAA;IACA;IACAe,KAAAC,MAAA;MACA,KAAAf,QAAA,CAAAgB,IAAA;QAAArB,UAAA,EAAAoB,MAAA;QAAAA,MAAA,OAAAzB,IAAA,CAAAO;MAAA,GAAAK,IAAA,CAAAC,GAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}